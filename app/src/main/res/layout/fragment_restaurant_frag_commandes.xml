<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:id="@+id/frameLayout2"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".restaurant.Restaurant_frag_commandes">


    <TextView
            android:id="@+id/restaurant_frag_commandes_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/mes_commandes"
            android:gravity="center"
            android:textAppearance="@style/TextAppearance.MaterialComponents.Headline6"
            app:layout_constraintTop_toTopOf="parent"
    />

    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/restaurant_frag_commandes_rv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/restaurant_frag_commandes_textView"
            android:layout_marginTop="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
